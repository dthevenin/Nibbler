<component xmlns="http://bender.igel.co.jp" href="View.xml" on-render="nibbler.ui.TextLabel">

  <property name="text"/>

  <view xmlns:html="http://www.w3.org/1999/xhtml" stack="replace">
    <html:div class="vs_ui_textlabel"></html:div>
  </view>

  <watch>
    <get property="text">
      this.__vs_instance.text = property;
    </get>
  </watch>

</component>
