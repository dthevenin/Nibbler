<component xmlns="http://bender.igel.co.jp" prototype="nibbler.ui.CheckBox">

  <property name="data" as="dynamic" value="[]"/>
  <property name="selectedIndexes" as="dynamic"/>
  <property name="model" as="dynamic"/>

  <view xmlns:html="http://www.w3.org/1999/xhtml">
    <html:div class="vs_ui_checkbox">
      <html:fieldset x-hag-hole="item_children" />
    </html:div>
  </view>
  
  <watch>
    <get instance="$self" property="data">
      this.__set_data(value)
    </get>

    <get instance="$self" property="model">
      this.__set_model(value)
    </get>
  </watch>

</component>
