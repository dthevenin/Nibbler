<component xmlns="http://bender.igel.co.jp" prototype="nibbler.data.GoogleSearch">

  <property name="searchEngine" as="dynamic" />
  <property name="address" as="string" />
  <property name="addresses" as="dynamic"/>
  <property name="position" as="dynamic" />
  
  <watch>
    <get instance="$self" property="searchEngine">
      this.__vs_instance.setSearchEngine (value);
    </get>

    <get instance="$self" property="address" value="this.__set_adress(value)" />

    <get instance="$self" property="position" value="this.__set_position(value)" />
  </watch>

</component>
